    "\n",
    "def is_happy(n):\n",
    "    seen_num = set()\n",
    "    while (n != 1) and (n not in seen_num):\n",
    "        seen_num.add(n)\n",
    "        n = sum([int(i) ** 2 for i in str(n)])\n",
    "    return n == 1\n",
    "        \n"
   ]
